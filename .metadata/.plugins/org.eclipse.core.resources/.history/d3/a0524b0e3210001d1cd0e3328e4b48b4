package aulas;

public class HelloWorld {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		/*Estudo sobre variaveis 18/07/22*/
		
		int minhaVariavel = 10; 
		String minhaString = "Paty";
		double minhaDouble = 10.25;
		char minhaChar = 'M';
		boolean verdadeiro = true;
		boolean falso = false;
		
		
		/*Primeiro programa java, para imprimir*/
		System.out.println("Hello World");
		
		/*Imprimir as variaveis*/
		System.out.println(minhaVariavel);
		System.out.println(minhaString);
		System.out.println(minhaDouble);
		System.out.println(minhaChar);
		System.out.println(verdadeiro);
		System.out.println(falso);
		System.out.println(minhaVariavel + " " + minhaString + minhaDouble);
	}

}
